{
  "rules": {
    "$id": {
      "pageData": {
        ".read": "true",
        ".write": "auth.uid == root.child($id).child('uid').val()"
      },
      "$other": {
        ".read": "auth.uid == root.child($id).child('uid').val()",
        ".write": "auth.uid == root.child($id).child('uid').val()"
      }
    }
  }
}
